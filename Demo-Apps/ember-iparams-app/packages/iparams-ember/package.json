{
  "name": "iparams-ember",
  "version": "0.0.1",
  "private": true,
  "description": "Ember app that runs in the custom iparams page.",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "clean": "rimraf ./dist ./tmp",
    "build:prod:ember": "ember build -p",
    "build:prod:copy": "rsync --recursive ./dist/jaya.css ./dist/jaya.js ./dist/vendor.css ./dist/vendor.js ./dist/assets/images/* ./dist/assets/scripts/* ./dist/assets/styles/* ../app/config/assets",
    "build:prod:src": "rsync -a app/ ../app/src/iparams",
    "build": "npm-run-all build:prod:*",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:fix'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js:fix": "eslint . --fix",
    "build:dev": "rsync --recursive ./dist/jaya.css ./dist/jaya.css.map ./dist/jaya.js ./dist/jaya.map ./dist/vendor.css ./dist/vendor.js ./dist/assets/images/* ./dist/assets/scripts/* ./dist/assets/styles/* ../app/config/assets",
    "watch": "concurrently \"ember build --watch\" \"watch 'yarn build:dev' ./dist\""
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-funnel": "^3.0.4",
    "broccoli-merge-trees": "^4.2.0",
    "concurrently": "^6.0.2",
    "ember-auto-import": "^1.11.2",
    "ember-cli": "~3.26.1",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-nprogress": "^0.3.1",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-intl": "^5.6.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.0.3",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.26.1",
    "ember-svg-jar": "^2.2.3",
    "ember-template-lint": "^3.2.0",
    "ember-toggle": "^7.1.1",
    "ember-tooltips": "^3.4.7",
    "ember-truth-helpers": "^3.0.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-ember": "^10.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.15.0",
    "qunit-dom": "^1.6.0",
    "rimraf": "^3.0.2",
    "rsync": "^0.6.1",
    "sass": "^1.32.11",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.0.0",
    "watch": "^1.0.2",
    "whatwg-fetch": "3.6.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "resolutions": {
    "whatwg-fetch": "3.3.1"
  },
  "dependencies": {
    "@freshworks/button": "^0.18.0",
    "@freshworks/toast-message": "^0.18.0"
  }
}
